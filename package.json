{
  "name": "teka-cli",
  "version": "1.0.3",
  "description": "Unofficial console client for Anilibria",
  "main": "./build/index.js",
  "type": "module",
  "bin": {
    "teka": "build/index.js"
  },
  "pkg": {
    "scripts": "./build/index.js",
    "targets": [
      "node16-windows-x64"
    ],
    "outputPath": "dist"
  },
  "scripts": {
    "prebuild": "rimraf build",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "pack": "pkg package.json",
    "format": "prettier --write ./src/",
    "lint": "eslint ./src/*",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add .",
    "postversion": "git push && git push --tags"
  },
  "keywords": [],
  "author": "Maxim \"maxqwars\" Maximenko <maxqwars@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.7.13",
    "@types/yargs": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.23.0",
    "pkg": "^5.8.0",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-typescript2": "^0.33.0",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@maxqwars/metaform": "^0.0.11",
    "cli-table3": "^0.6.2",
    "colorette": "^2.0.19",
    "cross-fetch": "^3.1.5",
    "fast-xml-parser": "^4.0.9",
    "millify": "^5.0.0",
    "yargs": "^17.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxqwars/teka-cli.git"
  },
  "bugs": {
    "url": "https://github.com/maxqwars/teka-cli/issues"
  },
  "homepage": "https://github.com/maxqwars/teka-cli#readme"
}
